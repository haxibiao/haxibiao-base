{
    "name": "haxibiao/breeze",
    "description": "Haxibiao 架构的基础包,从用户认证到基本内容社交系统，长视频短视频媒体内容系统...",
    "license": "MIT",
    "authors": [
        {
            "name": "zengdawei",
            "email": "zengdawei@haxibiao.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "haxibiao/helpers": "*",
        "haxibiao/config": "*",
        "haxibiao/content": "*"
    },
    "autoload": {
        "psr-4": {
            "Haxibiao\\Breeze\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Haxibiao\\Breeze\\tests\\": "tests/"
        }
    },
    "repositories": {
        "helpers": {
            "type": "vcs",
            "url": "https://github.com/haxibiao/haxibiao-helpers.git"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Haxibiao\\Breeze\\BreezeServiceProvider"
            ]
        }
    }
}
