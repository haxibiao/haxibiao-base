{
    "name": "haxibiao/breeze",
    "description": "Haxibiao 架构的基础包,从用户认证到基本内容社交系统，长视频短视频媒体内容系统...",
    "license": "MIT",
    "authors": [
        {
            "name": "zengdawei",
            "email": "zengdawei@haxibiao.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^8.0",
        "nuwave/lighthouse": "^5.0",
        "haxibiao/console": "*",
        "haxibiao/helpers": "*",
        "haxibiao/config": "*",
        "haxibiao/sns": "*",
        "haxibiao/wallet": "*",
        "haxibiao/dimension": "*",
        "haxibiao/task": "*",
        "mll-lab/laravel-graphql-playground": "^2.1",
        "haxibiao/content": "*"
    },
    "autoload": {
        "psr-4": {
            "Haxibiao\\Breeze\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/factories/",
            "database/seeder/",
            "tests/"
        ]
    },
    "repositories": {
        "helpers": {
            "type": "vcs",
            "url": "https://github.com/haxibiao/haxibiao-helpers.git"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Haxibiao\\Breeze\\BreezeServiceProvider"
            ]
        }
    }
}
